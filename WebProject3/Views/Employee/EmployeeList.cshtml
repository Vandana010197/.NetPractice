@model IEnumerable<WebProject3.Models.Employee>
@{
}

<h2>Employee Details</h2>
@if (Model.Count() <= 0)
            {
                <h3 class="text-center text-danger">Employee not found at this location</h3>
            }
            else { 
<table class="table table-bordered">
    <tr>
        
        <th>Name</th>
        <th>Email</th>
    </tr>
    @foreach (var emp in Model)
    {
        <tr>
            
            <td>@emp.Name</td>
            <td>@emp.Email</td>
        </tr>
    }
</table>
}